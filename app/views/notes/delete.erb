<p>Are you sure you want to delete:</p>
<blockquote><p><%= @note.title %></p></blockquote>
<form action="/note/<%= @note.id %>/destroy" method="post">
  <div>
    <button type="submit" class="btn btn-danger" name="ok">Delete</button>
    <button type="submit" class="btn btn-default" name="cancel">Cancel</button>
  </div>
</form>
<a href="/">Back</a>
